# E2E test user credentials
E2E_USER_EMAIL=test@example.com
E2E_USER_PASSWORD=testpassword123

# E2E test data identifiers
E2E_ORG_SLUG=test-org
E2E_FORM_SLUG=test-form

# Base URL (optional, defaults to localhost:3001)
E2E_BASE_URL=http://localhost:3001

# E2E API Configuration
# API URL for E2E test data management endpoints
E2E_API_URL=http://localhost:4000
# Secret for authenticating with E2E API (must match API's E2E_API_SECRET)
# Generate with: openssl rand -hex 32
E2E_API_SECRET=your-32-character-secret-here-change-me
